<html>
	<head>
		<link rel="stylesheet" href="/feedstyle.css">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
        <title>
			<% data.forEach(item => { %> - <%= item.name %> - <% }); %>
		</title>
    </head>
	<body>

		<% data.forEach(item => { %>
		<nav>
			<div class="nav-left">
				<img src="" class="logo" alt ="img logo">
				<ul>
					<li>
						<img src="" alt="notifications">
					</li>
					<li>
						<img src="" alt="mailbox">
					</li>
					<li>
						<img src="" alt="video">
					</li>
				</ul>
			</div>
			<div class="nav-right">
				<div class="search-box">
					<img src="images/search.png">
					<input type="text" placeholder="Search">
				</div>
				<div class="nav-user-icon online" onclick="settingsMenuToggle();">
					<img src="" alt="profile-pic">
				</div>
			</div>

			<div class="settings-menu">
				<div class="settings-menu-inner">
					<div class="user-profile">
						<img src="" alt ="profile-pic">
						<div>
                            <!------------------ table name goes here------------ -->
							<p><%= item.name %></p>
							<a href="#">See Your Profile</a>
						</div>
					</div>
					<div class="settings-links">
						<img src="" alt="settings" class="settings-icon">
						<a href="#">Settings</a>
					</div>
					<div class="settings-links">
						<img src="" alt="help" class="settings-icon">
						<a href="#">Help</a>
					</div>
					<div class="settings-links">
						<img src="" alt="logout" class="settings-icon">
						<a href="#">Logout</a>
					</div>
				</div>
			</div>
		</nav>

		<div class="container">
			<div class="left-sidebar">
				<div class="imp-links">
					<a href="#">
						<img src="images/friends.png">
						Friends
					</a>
					<a href="#">
						<img src="images/group.png">
						Groups
				</div>
			</div>
			<div class="main-content">
				<div class="story-gallery">
				</div>
				<div class="write-post-container">
					<div class="user-profile">
						<img src="" alt="profile-pic">
						<div></div>
                                    <!------------------ table name goes here------------ -->
							<p><%= item.name %></p>
						</div>
					</div>
					<div class="post-input-container">
                                  <!------------------ pet name goes where placeholder is------------ -->
						<textarea id="userInput" rows="3" placeholder="What's on your mind, <%= item.name %> ?"></textarea>
						<button onclick="userPosting()">Submit</button>
						<input type="file" accept="image/*" onchange="loadFile(event)">
						<div class="add-post-links">
							<a href="#">
								<img src="images/photo.png">
								Photo/Video
							</a>
						</div>
					</div>
				</div>
				<div class="post-container">
					<div class="post-row">
						<div class="user-profile">
							<img src="" alt="profile-pic">
							<div>
                            <!------------------ table name goes here------------ -->
								<p><%= item.name %></p>
								<span>Timestamp</span>
							</div>
						</div>
						<a href="#">
							<i class="fa fa-ellipsis-v"></i>
						</a>
					</div>
					<p id="userMessage"></p>

					<p><img id="output" width="200"/></p>

					<div class="post-row">
						<div class="activity-icons">
							<div>
								<img onclick="" src="images/like.png">
							</div>
							<div>
								<img src="images/comments.png">
							</div>
							<div>
								<img src="images/share.png">
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="right-sidebar">
				<div class="sidebar-title">
					<h4>Events</h4>
					<a href="#">See All</a>
				</div>
				<div class="event">
					<div class="left-event">
						<h3>4</h3>
						<span>July</span>
					</div>
					<div class="right-event">
						<h4>Pet 5k</h4>
						<p>
							<i class="fa fa-map-marker"></i>
							Piedmont Park
						</p>
						<a href="#">More Info</a>
					</div>
				</div>
				<div class="event">
					<div class="left-event">
						<h3>2</h3>
						<span>Sept</span>
					</div>
					<div class="right-event">
						<h4>Pet Cosplay Meetup</h4>
						<p>
							<i class="fa fa-map-marker"></i>
							Hyatt Regency
						</p>
						<a href="#">More Info</a>
					</div>
				</div>
			</div>
		</div>
		<div class="footer">
			<p>Copyright 2023 - Ben, Denis, and Jason Full Stack Project</p>
		</div>
		<% }); %>
		<script src="/feedscript.js"></script>
	</body>
</html>